{
  "name": "@lukethacoder/lwrjs-module-registry",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "version": "0.12.2",
  "homepage": "https://developer.salesforce.com/docs/platform/lwr/overview",
